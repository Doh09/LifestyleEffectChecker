<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LifestyleEffectChecker.Views.DetailViews.JournalDeletePage"
             Title="{Binding Title}">
    <StackLayout Spacing="20" Padding="15">
        <Label Text="{Binding Journal.Name}" FontSize="Medium"/>
        <Entry x:Name="DeletionConfirmationEntry"></Entry>
        <Button Text="Confirm deletion" Clicked="ConfirmDeletion_OnClicked"></Button>
    </StackLayout>
</ContentPage>