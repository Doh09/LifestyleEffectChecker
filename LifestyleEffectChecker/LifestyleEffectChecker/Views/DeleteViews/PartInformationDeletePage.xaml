<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LifestyleEffectChecker.Views.DeleteViews.Action.PartInformationDeletePage"
             Title="{Binding Title}">
    <StackLayout Spacing="20" Padding="15">
        <Label Text="{Binding Journal.Name}" FontSize="Medium"/>
        <Entry Text="" Placeholder="Enter Journal name here" x:Name="DeletionConfirmationEntry" PropertyChanged="DeletionConfirmationEntry_OnPropertyChanged"></Entry>
        <Label Text="InfoLabel" x:Name="MatchingNames"></Label>
        <Button Text="Confirm deletion" Clicked="ConfirmDeletion_OnClicked"></Button>
    </StackLayout>
</ContentPage>